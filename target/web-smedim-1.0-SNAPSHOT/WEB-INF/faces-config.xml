<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/publico/login.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>medico/mostrar</from-outcome>
            <to-view-id>/privado/cadastro/medico/mostrar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/privado/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>convenio</from-outcome>
            <to-view-id>/privado/cadastro/convenio/listagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>medico</from-outcome>
            <to-view-id>/privado/cadastro/medico/listagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>servico</from-outcome>
            <to-view-id>/privado/cadastro/servico/listagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>tabelaDePreco</from-outcome>
            <to-view-id>/privado/cadastro/tabelaDePreco/listagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>usuario</from-outcome>
            <to-view-id>/privado/cadastro/usuario/listagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>cliente</from-outcome>
            <to-view-id>/privado/cliente/listagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>agenda</from-outcome>
            <to-view-id>/privado/agenda/listagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>expediente</from-outcome>
            <to-view-id>/privado/expediente/listagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>prontuario</from-outcome>
            <to-view-id>/privado/medico/prontuario/listagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>faturamentoPorServico</from-outcome>
            <to-view-id>/privado/faturamento/listagemPorServico.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>faturamentoPorConvenio</from-outcome>
            <to-view-id>/privado/faturamento/listagemPorConvenio.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>faturamentoDiario</from-outcome>
            <to-view-id>/privado/faturamento/listagemDiario.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <lifecycle>
        <phase-listener>
            smedim.rn.AutenticacaoLogin
        </phase-listener>
        <phase-listener>
            smedim.config.LogFilter
        </phase-listener>
    </lifecycle>
</faces-config>
